{
  "name": "ladybug-hosting-v7",
  "version": "7.0.0",
  "description": "Ladybug hosting site with bot net servers - deployable on render.com",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required'",
    "build:client": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "deploy": "npm run build && git add . && git commit -m 'Deploy v7' && git push origin main"
  },
  "keywords": ["ladybug", "hosting", "bot-net", "render", "server"],
  "author": "Ladybug Hosting Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "socket.io": "^4.7.5",
    "mongoose": "^8.4.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.2.0",
    "dotenv": "^16.4.5",
    "axios": "^1.7.2",
    "winston": "^3.13.0",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}