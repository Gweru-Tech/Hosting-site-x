{
  "name": "ladybug-hosting-v7",
  "version": "7.0.0",
  "description": "Ladybug hosting site with bot net servers - deployable on render.com",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client",
    "build:client": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "deploy": "npm run build && git add . && git commit -m 'Deploy v7' && git push origin main"
  },
  "keywords": ["ladybug", "hosting", "bot-net", "render", "server"],
  "author": "Ladybug Hosting Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "mongoose": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "winston": "^3.10.0",
    "node-cron": "^3.0.2",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "eslint": "^8.48.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "file-loader": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}